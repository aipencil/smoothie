{
    "name": "aipencil/smoothie",
    "description": "A Filament Admin Agent skill package that extends Filament with reusable features, tools, and UI enhancements to speed up Laravel admin panel development.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aipencil\\Smoothie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aipencil\\Smoothie\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Shaung Bhone",
            "email": "phonelaayy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/support": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "rector/rector": "^2.0",
        "orchestra/testbench": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aipencil\\Smoothie\\SmoothieServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test:lint": [
            "pint --test"
        ],
        "test:unit": "pest",
        "test:types": "phpstan",
        "test": [
            "@test:unit",
            "@test:lint",
            "@test:types"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
